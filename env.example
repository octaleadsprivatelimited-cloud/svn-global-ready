# Server Configuration
NODE_ENV=production
PORT=3000

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# CORS Configuration (comma-separated list of allowed origins)
ALLOWED_ORIGINS=http://localhost:8080,http://localhost:3000,https://your-domain.com

# Frontend Environment Variables (for build)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key
VITE_SUPABASE_PROJECT_ID=your_supabase_project_id

# Optional: Use backend API instead of direct Supabase
# Set to true to use backend API endpoints
VITE_USE_BACKEND_API=false
VITE_API_URL=http://localhost:3000

